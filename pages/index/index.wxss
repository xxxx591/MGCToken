/**index.wxss**/



.submit {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 94rpx;
  display: flex;
  background-image: var(--gradualBlue);
  color: var(--white);
  align-items: center;
  text-align: center;
  margin-top: 50rpx;
}

.submit text {
  z-index: 999;
  width: 100%;
  line-height: 94rpx;
  background-image: var(--gradualBlue);
  color: var(--white);
}

.item-box {
  padding: 30rpx;
  align-items: center;
  font-size: 34rpx;
  line-height: 40rpx;
  display: flex;
  background: #fff;
  border-bottom: 1px solid #efefef;
}

.item-left {
  line-height: 40rpx;
  flex: 1;
  background: #fff;
}

.item-right {
  flex: 2;
  background: #fff;
}

.tips {
  font-size: 34rpx;
  line-height: 40rpx;
  background-image: var(--gradualBlue);
  color: var(--white);
  padding: 30rpx;
}
