/* pages/question/question.wxss */.submit {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 94rpx;
  display: flex;
  color: #fff;
  align-items: center;
  text-align: center;
  margin-top: 50rpx;
}

.submit text {
  z-index: 999;
